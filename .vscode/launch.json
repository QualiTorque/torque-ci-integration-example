    "version": "0.2.0",
    "configurations": [
        
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "End Sandbox",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\endSandbox.js",
            "args": ["${env:SANDBOX_ID}"],
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Start Sandbox",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\startSandbox.js"
        }
    ]
}